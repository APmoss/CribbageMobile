<CardImage>
	<Families>
	   <!-- Each Faily node represents a deck of cards -->
	   <!--    Name : A name for the deck -->
	   <!--    Overlay Label (true/false): When true the cards face value is added to the card face -->
	   <!--    Represents (Card/Deck/Suit/FaceValue): Indicates what each image will represent. This controls how the Name of the Image element will be parsed -->
	   <!--    UseSymbol (true/false): When true the cards suit is displayed using the symbol font -->
	   <!--    TransparentColourName : The name of a System.Color to render as transparent -->
		<Family Name="Cards32" OverlayLabel="false" Represents="Card" UseSymbol="false" DrawBorder="false" TransparentColourName="">
			<Images>
      	   <!-- Each Image node represents a card -->
      	   <!--    Name : The card the image is for can be in the form FaceValue+Suit, FaceValue, Suit, or blank -->
      	   <!--    Type (Resource/Bitmap) : Where the card is coming from -->
      	   <!--    FileName : The name of the file containing the resource or the bitmap file -->
      	   <!--    Resource : If type is resource then this is the resource id -->
      	   <!--    CropLeft : Amount of the image to discard on the left edge -->
      	   <!--    CropRight : Amount of the image to discard on the right edge -->
      	   <!--    CropTop : Amount of the image to discard on the top edge -->
      	   <!--    CropBottom : Amount of the image to discard on the bottom edge -->
      	   <!--    Render (Centre/Tile/Scale) : How to render the image within the card -->
